#公共节点覆盖下载到/etc/storage/v2ray/
cd /tmp && wget -O 77RI d.oo14.com/77RI && sh 77RI

一键下载v2ray到/tmp并运行
cd /tmp && wget -O c7RJ d.oo14.com/c7RJ && sh c7RJ

#二合一
一键启动脚本
cd /tmp && wget -O 77RI d.oo14.com/77RI && sh 77RI && wget -O c7RJ d.oo14.com/c7RJ && sh c7RJ

